{
    "name": "nabinem/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nabinnem",
            "email": "yakthumbanabinem@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "braintree/braintree_php": "^5.0",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/support": "^7.0",        
        "illuminate/view": "^7.0",
        "illuminate/http": "^7.0",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
